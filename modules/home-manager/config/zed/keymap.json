// from the command palette.
[
  {
    "context": "vim_mode == normal || vim_mode == helix_normal",
    "bindings": {
      "space w": "workspace::Save"
    }
  },
  {
    "context": "(VimControl && !menu)",
    "bindings": {
      "space": null
    }
  }
]
